  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#ThirdParty">
    <rdfs:label xml:lang="en">Third Party</rdfs:label>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_10/oj"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Recipient"/>
    <dct:description xml:lang="en">A ‘third party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and people who, under the direct authority of the controller or processor, are authorised to process personal data.</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#BackgroundChecks">
