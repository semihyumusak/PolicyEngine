  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#RepairImpairments">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:description xml:lang="en">Purposes associated with identifying, rectifying, or otherwise undertaking activities intended to fix or repair impairments to existing functionalities</dct:description>
    <rdfs:comment xml:lang="en">An example of identifying and rectifying impairments is the process of finding and fixing errors in products, commonly referred to as debugging</rdfs:comment>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-24</dct:created>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:label xml:lang="en">Repair Impairments</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#ServiceProvision"/>
  </rdf:Description>
