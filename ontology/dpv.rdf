    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <rdfs:comment xml:lang="en">Inferred Data is derived data generated from existing data, but which did not originally exist within it, e.g. inferring demographics from browsing history.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <dct:description xml:lang="en">Personal Data that is obtained through inference from other data</dct:description>
