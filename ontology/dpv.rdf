  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#InferredPersonalData">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#DerivedPersonalData"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#GeneratedPersonalData"/>
