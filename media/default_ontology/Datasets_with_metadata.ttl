# Define namespaces
@prefix ex: <http://example.org/datasets/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Dataset 1: COVID-19 Statistics
ex:covid19Stats rdf:type ex:Dataset ;
     "COVID-19 Statistics Dataset" ;
    dct:description "Dataset containing COVID-19 statistics from various countries." ;
    dct:publisher "World Health Organization" ;
    dct:issued "2024-02-01"^^xsd:date ;
    dct:license <http://creativecommons.org/licenses/by/4.0/> ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Country" ;
        ex:columnDataType "String" ;
        ex:columnDescription "Name of the country." ;
        ex:columnExample "United States"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Cases" ;
        ex:columnDataType "Integer" ;
        ex:columnDescription "Number of COVID-19 cases reported." ;
        ex:columnExample "10000"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Deaths" ;
        ex:columnDataType "Integer" ;
        ex:columnDescription "Number of deaths due to COVID-19." ;
        ex:columnExample "500"
    ] ;
    # Add more columns as needed
    .

# Dataset 2: Climate Change Data
ex:climateChangeData rdf:type ex:Dataset ;
    dct:title "Climate Change Data" ;
    dct:description "Dataset with climate-related data for research and analysis." ;
    dct:publisher "Intergovernmental Panel on Climate Change" ;
    dct:issued "2024-02-02"^^xsd:date ;
    dct:license <http://opensource.org/licenses/MIT> ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Location" ;
        ex:columnDataType "String" ;
        ex:columnDescription "Geographical location of data." ;
        ex:columnExample "United States"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Temperature" ;
        ex:columnDataType "Float" ;
        ex:columnDescription "Temperature data in Celsius." ;
        ex:columnExample "25.5"
    ] ;
    # Add more columns as needed
    .

# Dataset 3: Economic Indicators
ex:economicIndicators rdf:type ex:Dataset ;
    dct:title "Economic Indicators Dataset" ;
    dct:description "Dataset containing economic indicators and financial data." ;
    dct:publisher "International Monetary Fund" ;
    dct:issued "2024-02-03"^^xsd:date ;
    dct:license <http://www.gnu.org/licenses/gpl-3.0.html> ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Country" ;
        ex:columnDataType "String" ;
        ex:columnDescription "Name of the country." ;
        ex:columnExample "United States"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "GDP" ;
        ex:columnDataType "Float" ;
        ex:columnDescription "Gross Domestic Product in USD." ;
        ex:columnExample "200000"
    ] ;
    # Add more columns as needed
    .

# Dataset 4: Biodiversity Records
ex:biodiversityRecords rdf:type ex:Dataset ;
    dct:title "Biodiversity Records Dataset" ;
    dct:description "Dataset with records of plant and animal biodiversity." ;
    dct:publisher "Convention on Biological Diversity" ;
    dct:issued "2024-02-04"^^xsd:date ;
    dct:license <http://creativecommons.org/licenses/by/4.0/> ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Species" ;
        ex:columnDataType "String" ;
        ex:columnDescription "Name of the species." ;
        ex:columnExample "Tiger"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Population" ;
        ex:columnDataType "Integer" ;
        ex:columnDescription "Population count of the species." ;
        ex:columnExample "5000"
    ] ;
    # Add more columns as needed
    .

# Dataset 5: Social Media Analytics
ex:socialMediaAnalytics rdf:type ex:Dataset ;
    dct:title "Social Media Analytics Dataset" ;
    dct:description "Dataset containing analytics data from social media platforms." ;
    dct:publisher "Example Analytics Company" ;
    dct:issued "2024-02-05"^^xsd:date ;
    dct:license <http://opensource.org/licenses/MIT> ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Platform" ;
        ex:columnDataType "String" ;
        ex:columnDescription "Name of the social media platform." ;
        ex:columnExample "Twitter"
    ] ;
    ex:hasColumn [
        rdf:type ex:Column ;
        ex:columnName "Users" ;
        ex:columnDataType "Integer" ;
        ex:columnDescription "Number of users on the platform." ;
        ex:columnExample "1000000"
    ] ;
    # Add more columns as needed
    .
